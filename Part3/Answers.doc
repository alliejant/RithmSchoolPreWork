	Part 3:
	function rockPaperScissors(){
  let randNum = Math.random();
  let compAns;
  
  if(randNum < .33) compAns = "rock";
  else if (randNum < .66) compAns = "paper";
  else compAns = "scissors";
  
  let userAns = prompt("Rock, paper, or Scissors?").toLowerCase();
  
  if(userAns === compAns) return "You tie"
  else if (userAns === "rock") {
    if(compAns === "paper") return "Comp wins"
    if(compAns === "scissors") return "You win"
  } else if (userAns === "paper") {
    if(compAns === "rock") return "You wins"
    if(compAns === "scissors") return "Comp win"
  }  else if (userAns === "scissors") {
    if(compAns === "rock") return "Comp wins"
    if(compAns === "paper") return "You win"
  } else return "There was a mistake somewhere, try again";
}

		


